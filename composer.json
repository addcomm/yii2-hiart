{
    "name": "addcomm/yii2-hiart",
    "type": "yii2-extension",
    "description": "ActiveRecord for API",
    "keywords": [
        "API",
        "ActiveRecord",
        "extension",
        "yii2",
        "rest"
    ],
    "homepage": "https://github.com/addcomm/yii2-hiart",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@addcomm.com",
        "source": "https://github.com/addcomm/yii2-hiart",
        "issues": "https://github.com/addcomm/yii2-hiart/issues",
        "wiki": "https://github.com/addcomm/yii2-hiart/wiki",
        "forum": "http://forum.addcomm.com/yii2"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@addcomm.com",
            "homepage": "http://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "role": "Lead backend developer",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        },
        {
            "name": "Andrey Klochok",
            "role": "Lead frontend developer",
            "email": "andreyklochok@gmail.com",
            "homepage": "http://addcomm.com/"
        },
        {
            "name": "Yuriy Myronchuk",
            "role": "QA Lead",
            "email": "bladeroot@gmail.com",
            "homepage": "http://addcomm.com/"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2": "dev-master",
        "hiqdev/yii2-hiart-github": "dev-master",
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\hiart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hiqdev\\hiart\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
